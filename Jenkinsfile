pipeline{
  agent any
  stages {
    stage('stage1'){
      steps {
        echo "Test  for Jenkinfile"
      }
    }
    stage('stage2'){
      steps {
        echo "The Jenkins file is Working!!!"
      }
    }
    stage('stage3'){
      steps {
        echo "Now let's build a real CICD pipeline"
      }
    }
  }
}    
